
th, td, body, input, p, textarea, select
	{
	font-family: "Tahoma", "Verdana", "Arial", "Geneva", "Times", "Helvetica";
	font-size: 11px;
	}


body
	{
	padding-left: 4px;
	padding-top: 4px;
	color: black;
	background: #ffffff;
	border-left: 1px solid #E2E0E4;
	border-right: 1px solid #736F77;
	border-top: 1px solid #E2E0E4;
	border-bottom: 1px solid #736F77;
	}


.pagetitle
	{
	letter-spacing: 2pt;
	font-weight: bold;
	background-color: #000000;
	color: #FFFFFF;
	text-align: left;
	}


.subHeader
	{
	color: #FFFFFF;
	letter-spacing: 2pt;
	font-weight: bold;
	padding-left: 5px;
	padding-right: 15px;
	background-color: #08597B;
	border-bottom: 1px solid #04354A;
	}

.menuitem
	{
	background-color: #F0ECDA;
	color: #897159;
	cursor: hand;
	}

.successMessage
	td{
		background-color:#f1f7f7;
		border:1px green solid;
		font-weight:bold;
		width: 100%;
	}
	
.errorMessage
	td{
		background-color:#B40404;
		border:1px green solid;
		font-weight:bold;
		width: 100%;
	}	

.bullet-links a	{
	display: inline-block;
	padding: 4px;
	outline: 0;
	color: #3a599d;
}
.bullet-links a:hover {
	background: #3a599d;
	text-decoration: none;
	color: #fff;
	border-radius: 4px;
}	
	
.menuitem a { color: #633100; }
.menuitemspan { color: #633100; }
